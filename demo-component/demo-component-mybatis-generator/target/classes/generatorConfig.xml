<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="demo" targetRuntime="MyBatis3DynamicSqlV2">
        <property name="targetJava8" value="false"/>
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
<!--        <plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin" />-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="com.hwj.demo.component.mybatis.generator.plugin.JSR303Plugin" />
<!--        <plugin type="AuditEntityPlugin" />-->
<!--        <plugin type="IgnoreAuditFiledInEntityPlugin" />-->

        <commentGenerator type="com.hwj.demo.component.mybatis.generator.plugin.LessCommentGenerator">
            <property name="suppressAllComments" value="false"/>
            <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>
<!--        <jdbcConnection driverClass="org.h2.Driver"-->
<!--                        connectionURL="jdbc:h2:~/test;DB_CLOSE_ON_EXIT=FALSE" userId="sa" password="sa"/>-->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://172.18.169.55:3306/sys?useUnicode=true;characterEncoding=utf-8" userId="root" password="admin"/>
        <javaTypeResolver>
            <property name="useJSR310Types" value="true" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.bosssoft.nontax3.saas.user.center.server.entity" targetProject="src/main/java">
            <property name="rootClass" value="com.bosssoft.nontax3.saas.component.audit.model.AuditSupport"/>
        </javaModelGenerator>

        <javaClientGenerator targetPackage="com.bosssoft.nontax3.saas.user.center.server.dao.base" targetProject="src/main/java"/>

        <table tableName="auth_user" mapperName="UserBaseMapper">
            <domainObjectRenamingRule searchString="^Auth" replaceString="" />
        </table>
    </context>
</generatorConfiguration>
